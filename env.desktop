# Desktop Environment Configuration for Windows
NODE_ENV=production
PORT=3000

# Storage Configuration (Local storage - unlimited!)
STORAGE_MODE=local

# Admin Configuration
ADMIN_PASSWORD=admin123

# Session Secret
SESSION_SECRET=Yourawaveydude4145!!!!

# Stem Processing Configuration (ENABLED!)
ENABLE_STEM_PROCESSING=true
PYTHON_PATH=python

# Processing Configuration (Optimized for 32GB RAM + i9)
STEM_PROCESSING_TIMEOUT=1800000  # 30 minutes (plenty of time)
MAX_CONCURRENT_PROCESSING=4      # Can handle multiple at once with 32GB RAM

# Desktop Optimizations
MAX_FILE_SIZE=1000000000         # 1GB max file size
DEMUCS_MEMORY_EFFICIENT=false    # Use full power mode with 32GB RAM
DEMUCS_JOBS=8                    # Use multiple CPU cores (i9 has many)

# Local Network Access
HOSTING_URL=http://localhost:3000
ALLOW_EXTERNAL_ACCESS=true       # Allow access from other devices on network

# Optional: External Access (uncomment to enable)
# NGROK_AUTH_TOKEN=your_ngrok_token
# ENABLE_NGROK=true 